          {
            "node": "Upload to Supabase Storage",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Upload to Supabase Storage": {
      "main": [
        [
          {
            "node": "Build CDN URL",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Build CDN URL": {
      "main": [
        [
          {
            "node": "Update Image Cache",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "settings": {
    "executionOrder": "v1"
  },
  "staticData": null,
